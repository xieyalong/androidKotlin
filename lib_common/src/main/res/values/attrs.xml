<?xml version="1.0" encoding="utf-8"?>
<resources>

   
    <declare-styleable name="HeadLayout">
        <attr name="title_tv1_text" format="string" />
        <attr name="title_tv2_text" format="string" />
        <attr name="right_tv_text" format="string" />
        <attr name="left_iv_visibility" format="boolean" />
        <attr name="title_tv2_visibility" format="boolean" />
        <attr name="title_iv_visibility" format="boolean" />
        <attr name="right_tv_visibility" format="boolean" />
        <attr name="right_iv1_visibility" format="boolean" />
        <attr name="right_iv2_visibility" format="boolean" />
        <attr name="right_iv1_src" format="reference" />
        <attr name="right_iv2_src" format="reference" />
        <attr name="title_iv_src" format="reference" />
    </declare-styleable>
    <declare-styleable name="RImageView">
        <attr name="riv_type" format="enum">
            <enum name="riv_circle" value="1"/>
            <enum name="riv_round" value="2"/>
        </attr>
        <attr name="riv_radius" format="dimension"/>
    </declare-styleable>
</resources>